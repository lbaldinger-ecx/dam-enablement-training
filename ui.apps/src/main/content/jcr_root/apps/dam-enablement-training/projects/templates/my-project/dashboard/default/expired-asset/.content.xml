<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:mixinTypes="[sling:Resource]"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="cq/gui/components/projects/admin/dashboard/tile"
        class="cq-projects-Pod-assetPod"
        order="{Long}5">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="cq/gui/components/projects/admin/dashboard/layouts/tile"/>
        <header jcr:primaryType="nt:unstructured">
            <title
                jcr:primaryType="nt:unstructured"
                jcr:title="Expired Assets"/>
            <actions jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <configurepod
                        granite:rel="asset-pod-configuration-activator"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/hyperlink"
                        text="Configuration..."/>
                </items>
            </actions>
        </header>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="cq/gui/components/projects/admin/dashboard/tiles/assets/body"/>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="cq/gui/components/projects/admin/dashboard/tiles/assets/footer"/>
    </jcr:content>
</jcr:root>
